<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<template id="assets_frontend_popup"
			inherit_id="website.assets_frontend" name="Favorites Cart">
			<xpath expr="." position="inside">
				<script type="text/javascript"
					src="/delivery_address/static/src/js/address_selection.js" />
				<script type="text/javascript"
					src="/delivery_address/static/src/js/website_country_change.js" />
			</xpath>
		</template>

		<template id="address_popup" name="Show Fav Modal">
			<t t-call="website.layout">
				<div id="myaddress" class="modal fade">
					<div class="modal-dialog">
						<div class="modal-content">
							<div class="modal-header">
								<button type="button" class="close" data-dismiss="modal"
									aria-hidden="true">times; </button>
							</div>
							<div class="modal-body">
								<button type="button" class="btn btn-danger"
									data-dismiss="modal">Close
								</button>
							</div>
						</div>
					</div>
				</div>
			</t>
		</template>
	</data>
</odoo>
